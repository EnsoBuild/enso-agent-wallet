{
  "name": "@lit-protocol/agent-toolkit",
  "version": "0.1.0",
  "dependencies": {
    "ethers": "catalog:",
    "openai": "^4.77.0",
    "zod": "^3.24.1"
  },
  "type": "commonjs",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "typings": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "require": "./dist/src/index.js",
      "default": "./dist/src/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "packages/agent-toolkit/src",
    "projectType": "library",
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/agent-toolkit/dist",
          "main": "packages/agent-toolkit/src/index.ts",
          "tsConfig": "packages/agent-toolkit/tsconfig.lib.json",
          "assets": [
            "packages/agent-toolkit/*.md"
          ]
        }
      }
    },
    "name": "agent-toolkit"
  }
}
