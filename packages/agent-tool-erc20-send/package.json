{
  "name": "@lit-protocol/agent-tool-erc20-send",
  "version": "0.0.1",
  "dependencies": {
    "@lit-protocol/agent-toolkit": "workspace:*",
    "ethers": "catalog:",
    "tslib": "^2.6.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "esbuild": "^0.19.11"
  },
  "type": "commonjs",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "typings": "./dist/src/index.d.ts",
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "sourceRoot": "packages/agent-tool-erc20-send/src",
    "projectType": "library",
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/agent-tool-erc20-send/dist",
          "main": "packages/agent-tool-erc20-send/src/index.ts",
          "tsConfig": "packages/agent-tool-erc20-send/tsconfig.lib.json",
          "assets": [
            "packages/agent-tool-erc20-send/*.md"
          ]
        }
      },
      "build:action": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            "mkdir -p artifacts",
            "node tools/scripts/build-lit-action.js"
          ],
          "cwd": "packages/agent-tool-erc20-send",
          "parallel": false
        },
        "outputs": [
          "{workspaceRoot}/packages/agent-tool-erc20-send/artifacts"
        ]
      }
    },
    "name": "agent-tool-erc20-send"
  }
}
